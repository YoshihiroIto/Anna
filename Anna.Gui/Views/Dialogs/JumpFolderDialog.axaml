<Window x:Class="Anna.Gui.Views.Dialogs.JumpFolderDialog"
        Classes="dialog"
        xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:base="clr-namespace:Anna.Gui.Views.Dialogs.Base"
        xmlns:behaviors="clr-namespace:Anna.Gui.Views.Behaviors"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:dialogs="clr-namespace:Anna.Gui.Views.Dialogs"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:messaging="clr-namespace:Anna.Gui.Views.Behaviors.Messaging"
        Title="{ReflectionBinding R.AppName}"
        d:DesignHeight="450"
        d:DesignWidth="800"
        x:CompileBindings="True"
        x:DataType="dialogs:JumpFolderDialogViewModel"
        mc:Ignorable="d">
    <Design.DataContext>
        <!--  ReSharper disable once Xaml.ConstructorWarning  -->
        <dialogs:JumpFolderDialogViewModel />
    </Design.DataContext>

    <Window.Styles>
        <StyleInclude Source="/Views/Resources/Styles.axaml" />
    </Window.Styles>

    <Interaction.Behaviors>
        <behaviors:DialogBehavior />
        <messaging:InteractionMessageTrigger MessageKey="{x:Static base:DialogViewModel.MessageKeyClose}" Messenger="{Binding Messenger}">
            <messaging:WindowInteractionMessageAction />
        </messaging:InteractionMessageTrigger>
    </Interaction.Behaviors>
    Jump Folder
</Window>